{
  "name": "testapi2",
  "version": "2.1.17",
  "description": "Test and auto export document to .md or .swagger file",
  "main": "dist/index.js",
  "bin": {
    "testapi2": "./bin/index.js"
  },
  "scripts": {
    "pub": "npm publish",
    "test": "node .",
    "start": "nodemon --watch src --watch modules --ext .ts --exec 'node -r ts-node/register -r ./bin/tsconfig-paths-bootstrap.js --expose-gc --harmony src/index.ts test/examples/crawler.yaml'",
    "dev": "nodemon --watch src --watch modules --ext .ts --exec 'node -r ts-node/register -r ./bin/tsconfig-paths-bootstrap.js --expose-gc --harmony --inspect src/index.ts /home/thanh/src/gitlab-gapo/user-relation-api-bit/testapi2'",
    "build": "yarn clean && tsc && yarn build:resolve-module",
    "build:resolve-module": "node bin/tsconfig",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "form-data": "^3.0.0",
    "js-yaml": "^3.12.2",
    "jsonschema": "^1.2.10",
    "lodash": "^4.17.11",
    "node-fetch": "^2.6.1",
    "pako": "^1.0.11",
    "to-json-schema": "^0.2.5"
  },
  "author": "doanthuanthanh88@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/chalk": "^2.2.0",
    "@types/js-yaml": "^3.12.1",
    "@types/lodash": "^4.14.122",
    "@types/node": "^7.10.5",
    "@types/node-fetch": "^2.5.7",
    "@types/pako": "^1.0.1",
    "@types/to-json-schema": "^0.2.0",
    "nodemon": "^2.0.2",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  }
}

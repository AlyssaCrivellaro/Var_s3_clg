{
    "editor.fontSize": 16,
    "workbench.iconTheme": "vs-seti",
    "files.autoSave": "off",
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "yaml.schemas": {
        "https://raw.githubusercontent.com/doanthuanthanh88/api-testing-doc/v5/schema.json": "*.yaml"
    },
    "yaml.customTags": [
        "!api mapping",
        "!get mapping",
        "!post mapping",
        "!put mapping",
        "!patch mapping",
        "!delete mapping",
        "!head mapping",
        "!echo scalar",
        "!echo-schema scalar",
        "!pause mapping",
        "!validate mapping",
        "!utils mapping",
        "!group mapping",
        "!script scalar",
        "!vars mapping",
        "!regex mapping",
        "!file scalar",
        "!upload scalar",
        "!doc-swagger mapping",
        "!doc-md mapping",
        "!output-file mapping",
        "!exec mapping",
        "!wrk mapping",
        "!input mapping",
        "!status scalar",
        "!status-text scalar"
    ],
    "files.exclude": {
        // "**/.git": true,
        // "**/.svn": true,
        // "**/.hg": true,
        // "**/CVS": true,
        // "**/.DS_Store": true,
        // "**/tslint.json": true,
        // "**/tsfmt.json": true,
        // "**/tsconfig.json": true
    }
}